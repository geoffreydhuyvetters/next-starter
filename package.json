{
  "name": "next-starter",
  "version": "0.1.0",
  "description": "A personal next starter project",
  "author": "Geoffrey Dhuyvetters",
  "license": "MIT",
  "dependencies": {
    "next": "^9.5.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-query": "^2.23.1",
    "zustand": "^3.1.3"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "format": "prettier . --ignore-path .gitignore --write",
    "lint:check": "eslint .",
    "types:check": "tsc",
    "format:check": "prettier . --ignore-path .gitignore --check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn lint && yarn types:check"
    }
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.0.3"
  }
}
